{
	"name": "shamtool.com-2",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"build:dev": "cross-env FOR_DEV=true svelte-kit build",
		"build:gh": "cross-env TO_GITHUB=true svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"deploy:gh": "node -r dotenv/config ./scripts/publish-gh-pages.mjs dotenv_config_path=./scripts/.env"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.56",
		"@sveltejs/adapter-static": "^1.0.0-next.22",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.0",
		"autoprefixer": "^10.3.7",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.8",
		"dotenv": "^10.0.0",
		"gh-pages": "^3.2.3",
		"postcss": "^8.3.9",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"tailwindcss": "^3.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"svelte-hero-icons": "^3.1.2",
		"svelte-use-click-outside": "^1.0.0"
	}
}
