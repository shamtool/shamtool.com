{
    "name": "shamtool.com-2",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "build:static": "cross-env IS_STATIC=true vite build",
        "build:gh": "cross-env TO_GITHUB=true vite build",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
        "format": "prettier --write --plugin-search-dir=. ./src",
        "format:all": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "deploy:gh": "node -r dotenv/config ./scripts/publish-gh-pages.mjs dotenv_config_path=./scripts/.env"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^1.0.0-next.96",
        "@sveltejs/adapter-static": "^1.0.0-next.43",
        "@sveltejs/kit": "1.0.0-next.504",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.7",
        "autoprefixer": "^10.4.12",
        "cross-env": "^7.0.3",
        "cssnano": "^5.1.13",
        "dotenv": "^16.0.2",
        "gh-pages": "^4.0.0",
        "postcss": "^8.4.16",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.1",
        "svelte": "^3.50.1",
        "svelte-check": "^2.9.1",
        "svelte-preprocess": "^4.10.7",
        "tailwindcss": "^3.1.8",
        "tslib": "^2.4.0",
        "typescript": "^4.8.3",
        "vite": "^3.1.3"
    },
    "type": "module",
    "dependencies": {
        "@steeze-ui/heroicons": "^2.1.0",
        "@steeze-ui/svelte-icon": "^1.3.2",
        "@types/prettier": "^2.7.1",
        "svelte-use-click-outside": "^1.0.0"
    }
}
